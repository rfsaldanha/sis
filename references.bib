@article{rosterMachineLearningBasedForecastingDengue2022,
  title = {Machine-{{Learning-Based Forecasting}} of {{Dengue Fever}} in {{Brazilian Cities Using Epidemiologic}} and {{Meteorological Variables}}},
  author = {Roster, Kirstin and Connaughton, Colm and Rodrigues, Francisco A.},
