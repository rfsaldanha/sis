@article{Barcellos1996,
  title = {Geoprocessamento, Ambiente e Sa{\'u}de: Uma Uni{\~a}o Poss{\'i}vel?},
  author = {Barcellos, Christovam and Bastos, Francisco In{\'a}cio},
  year = {1996},
