@article{ezeAnomalyDetectionEndemic2023,
  title = {Anomaly {{Detection}} in {{Endemic Disease Surveillance Data Using Machine Learning Techniques}}},
  author = {Eze, Peter U. and Geard, Nicholas and Mueller, Ivo and Chades, Iadine},
  year = {2023},
