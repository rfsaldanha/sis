@article{levesquePatientcentredAccessHealth2013,
  title = {Patient-Centred Access to Health Care: Conceptualising Access at the Interface of Health Systems and Populations},
  shorttitle = {Patient-Centred Access to Health Care},
  author = {Levesque, Jean-Frederic and Harris, Mark F and Russell, Grant},
